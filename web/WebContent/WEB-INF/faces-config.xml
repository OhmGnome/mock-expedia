<?xml version="1.0"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
  <application>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 </application>
 
 <navigation-rule>
  <!-- always starts with a view -->
  <from-view-id>/*</from-view-id>
    <navigation-case>
   <from-outcome>travel</from-outcome>
   <to-view-id>/secured/travel.xhtml</to-view-id>
   <!--    <redirect/> -->
  </navigation-case>
  <navigation-case>
   <from-outcome>schedule</from-outcome>
   <to-view-id>/secured/schedule.xhtml</to-view-id>
   <!--    <redirect/> -->
  </navigation-case>
  <navigation-case>
   <from-outcome>profile</from-outcome>
   <to-view-id>/secured/profile.xhtml</to-view-id>
   <!--    <redirect/> -->
  </navigation-case>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/home.xhtml</to-view-id>
   <!--    <redirect/> -->
  </navigation-case>
  <navigation-case>
   <from-outcome>registrar</from-outcome>
   <to-view-id>/registrar.xhtml</to-view-id>
   <!--    <redirect/> -->
  </navigation-case>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
